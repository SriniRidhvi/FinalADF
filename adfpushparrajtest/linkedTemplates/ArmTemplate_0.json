								"parameters": {
									"Asset_Id": {
										"value": "@pipeline().parameters.Asset_ID",
										"type": "Expression"
									},
									"Folder_Name": {
										"value": "@concat('content/asset/print/',pipeline().parameters.Asset_ID)",
										"type": "Expression"
									}
								}
							},
							"firstRowOnly": false
						}
					},
					{
						"name": "ForEach",
						"type": "ForEach",
						"dependsOn": [
							{
								"activity": "Read_Json_From_blob",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"items": {
								"value": "@activity('Read_Json_From_blob').output.value",
								"type": "Expression"
							},
							"isSequential": true,
							"activities": [
								{
									"name": "Download_Image_to_Blob",
									"type": "Copy",
									"dependsOn": [],
									"policy": {
										"timeout": "7.00:00:00",
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"source": {
											"type": "BinarySource",
											"storeSettings": {
												"type": "HttpReadSettings",
												"requestMethod": "GET"
											},
											"formatSettings": {
												"type": "BinaryReadSettings"
											}
										},
										"sink": {
											"type": "BinarySink",
											"storeSettings": {
												"type": "AzureBlobStorageWriteSettings"
											}
										},
										"enableStaging": false
									},
									"inputs": [
										{
											"referenceName": "ds_binary_http_src",
											"type": "DatasetReference",
											"parameters": {
												"BinaryFileName": {
													"value": "@activity('Read_Json_From_blob').output.value[0].properties.FileName",
													"type": "Expression"
												}
											}
										}
									],
									"outputs": [
										{
											"referenceName": "ds_binary_blob_sink",
											"type": "DatasetReference",
											"parameters": {
												"File_Name_1": {
													"value": "@activity('Read_Json_From_blob').output.value[0].properties.FileName",
													"type": "Expression"
												},
