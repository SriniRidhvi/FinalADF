							],
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "Child_Get_Json_Files_in_Asset_File",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"Asset_Id": {
